  "resolutions": {
    "@types/react": "^16.14.20",
    "@types/react-dom": "^16.9.14"
  },
  "bin": {
    "generate_scheme": "./tasks/generate_scheme.js"
  },
  "peerDependencies": {
    "@vkontakte/icons": "^1.184.0",
    "@vkontakte/vk-bridge": "^2.0.2",
    "react": "^16.8.6 || ^17.0.0 || ^18.1.0",
    "react-dom": "^16.8.6 || ^17.0.0 || ^18.1.0"
  },
  "size-limit": [
    {
      "name": "JS",
      "path": "dist/index.js",
      "gzip": false
    },
    {
      "name": "JS (gzip)",
      "path": "dist/index.js"
    },
    {
      "name": "JS (brotli)",
      "path": "dist/index.js",
      "brotli": true
    },
    {
      "name": "JS, unstable",
      "path": "dist/unstable/index.js"
    },
    {
      "name": "CSS",
      "path": "dist/vkui.css",
      "webpack": false,
      "gzip": false
    },
    {
      "name": "CSS (gzip)",
      "path": "dist/vkui.css",
      "webpack": false
    },
    {
      "name": "CSS (brotli)",
      "path": "dist/vkui.css",
      "webpack": false,
      "brotli": true
    },
    {
      "name": "CSS, unstable",
      "path": "dist/unstable.css",
      "webpack": false
    }
  ],
  "scripts": {
    "prepublishOnly": "yarn build",
    "size": "yarn clear && concurrently 'yarn:babel' 'yarn:postcss' && size-limit",
    "size:ci": "yarn install --frozen-lockfile --ignore-scripts && yarn build:no-types",
    "styleguide": "cross-env NODE_ENV=development styleguidist server --config=styleguide/config.js",
    "styleguide:props": "VKUI_STYLEGUIDE_PROPSPARSER=1 yarn styleguide",
    "styleguide:build": "cross-env NODE_ENV=production styleguidist build --config=styleguide/config.js",
    "dev": "yarn clear && concurrently \"yarn:tsc-dev\" \"yarn:babel-dev\" \"yarn:postcss-dev\"",
    "dev-cjs": "yarn clear && concurrently \"yarn:babel-cjs-dev\" \"yarn:postcss-dev\"",
    "dev-all": "yarn clear && concurrently \"yarn:tsc-dev\" \"yarn:babel-dev\" \"yarn:babel-cjs-dev\" \"yarn:postcss-dev\"",
    "postcss-bundle": "webpack --config webpack.styles.config.js",
    "postcss-modules": "postcss './src/**/*.css' --base ./src --dir ./dist/cssm",
    "postcss-dev": "concurrently 'yarn:postcss-bundle --watch' 'yarn:postcss-modules --watch'",
    "babel-base": "babel src/ --extensions .tsx,.jsx,.ts,.js",
    "babel-dev": "yarn babel-base --out-dir dist/ --watch",
    "babel-cjs-dev": "yarn babel-base --config-file ./babel.cjs.config.js --out-dir dist/cjs/ --watch",
    "tsc-dev": "tsc --emitDeclarationOnly --declaration --outDir dist/ --watch --preserveWatchOutput",
    "build:types": "yarn tsc && yarn tsc-cssm",
    "build:no-types": "concurrently 'yarn:babel' 'yarn:babel-cssm' 'yarn:babel-cjs' 'yarn:postcss'",
    "build": "yarn build:types && yarn build:no-types",
    "postcss": "cross-env NODE_ENV=production concurrently 'yarn:postcss-bundle' 'yarn:postcss-modules'",
    "babel": "cross-env NODE_ENV=production yarn babel-base --out-dir dist/ --source-maps",
    "babel-cssm": "cross-env BABEL_KEEP_CSS=1 NODE_ENV=production yarn babel-base --out-dir dist/cssm --source-maps",
    "babel-cjs": "cross-env NODE_ENV=production yarn babel-base --config-file ./babel.cjs.config.js --out-dir dist/cjs/ --source-maps",
    "tsc": "cross-env NODE_ENV=production tsc --emitDeclarationOnly --declaration -p tsconfig.dist.json",
    "tsc-cjs": "cross-env NODE_ENV=production tsc --emitDeclarationOnly --declaration --outDir dist/cjs/ -p tsconfig.dist.json",
    "tsc-cssm": "cross-env NODE_ENV=production tsc --emitDeclarationOnly --declaration --outDir dist/cssm/ -p tsconfig.dist.json",
    "clear": "shx rm -rf dist/*",
    "test:unit": "jest --config jest.unit.config.js",
    "test:unit:ci": "yarn test:unit --ci --silent --outputFile test-results.json --json --coverage --coverageReporters='json' --coverageDirectory='.nyc_output'",
    "test:e2e": "jest --config e2e/jest.e2e.config.js",
    "test:e2e:ci": "yarn test:e2e --ci --outputFile e2e-results.json --json",
    "test:combine-coverage": "npx nyc report --reporter=lcov",
    "lint:es": "eslint --ext .jsx,.js,.ts,.tsx src/ e2e/ styleguide/ --cache --cache-location .cache/.eslintcache",
    "lint:es:ci": "yarn lint:es -o lint-results.json -f json",
    "lint:style": "cross-env LINT_PRECOMMIT_RUN=1 stylelint \"./src/**/*.css\" --cache --cache-location .cache/.stylelintcache",
    "lint:prettier": "prettier --check --ignore-unknown .",
    "typecheck": "tsc --incremental --emitDeclarationOnly --declaration --tsBuildInfoFile .cache/.tsbuildinfo --outDir .cache/ts",
    "test:static": "concurrently 'yarn typecheck' 'yarn lint:es' 'yarn lint:style' 'yarn lint:prettier'",
    "test:static:ci": "concurrently 'yarn typecheck' 'yarn lint:es:ci' 'yarn lint:style' 'yarn lint:prettier'",
    "test": "concurrently 'yarn test:unit' 'yarn test:static'",
    "test:ci": "concurrently 'yarn test:unit:ci' 'yarn test:static:ci'",
    "codesandbox:install": "PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD=1 yarn install",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --check",
      "eslint"
    ],
    "*.css": [
      "prettier --check",
      "cross-env LINT_PRECOMMIT_RUN=1 stylelint"
    ],
    "*.{json,md}": [
      "prettier --check"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "@popperjs/core": "^2.9.2",
    "@vkontakte/vkjs": "^0.23.0",
    "dayjs": "^1.11.0",
    "mitt": "^3.0.0",
    "react-popper": "^2.2.5"
  },
  "engines": {
    "yarn": "^1.21.1"
  }
}
