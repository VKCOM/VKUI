  "scripts": {
    "docs:styleguide": "yarn workspace @project-docs/styleguide run start",
    "docs:styleguide:props": "yarn workspace @project-docs/styleguide run start:props",
    "docs:styleguide:build": "yarn workspace @project-docs/styleguide run build",
    "docs:storybook": "yarn workspace @vkontakte/vkui storybook",
    "build": "yarn workspaces run build",
    "build:vkui": "yarn workspace @vkontakte/vkui run build",
    "test": "yarn workspaces run test",
    "test:ci": "yarn workspaces run test:ci",
    "test:a11y": "yarn workspace @vkontakte/vkui run test:a11y",
    "test:a11y:ci": "yarn workspace @vkontakte/vkui run test:a11y:ci",
    "//": "Note ('test:e2e'|'test:e2e:ci'): не вызываем через 'yarn workspace', т.к. 'jest-playwright-preset' создаёт .nyc_output/ в папке e2e/, а нам надо, чтобы в корне проекта лежало. Настройки для изменения поведения нет (справедливо для '<= 3.0.1').",
    "test:e2e": "jest --config e2e/jest.e2e.config.js",
    "test:e2e:ci": "yarn test:e2e --ci --outputFile e2e-results.json --json",
    "test:coverage": "npx nyc report --reporter=lcov",
    "lint": "concurrently 'yarn:lint:types' 'yarn:lint:es' 'yarn:lint:style' 'yarn:lint:prettier'",
    "lint:es": "eslint . --ext .jsx,.js,.ts,.tsx --cache --cache-location .cache/.eslintcache",
    "lint:es:ci": "yarn lint:es -o lint-results.json -f json",
    "lint:style": "cross-env LINT_PRECOMMIT_RUN=1 stylelint '**/*.css' --cache --cache-location .cache/.stylelintcache",
    "lint:prettier": "prettier --check --ignore-unknown .",
    "lint:types": "tsc --incremental --emitDeclarationOnly --declaration --tsBuildInfoFile .cache/.tsbuildinfo --outDir .cache/ts",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "playwright:install": "playwright install --with-deps",
    "codesandbox:install": "PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD=1 yarn install"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --check",
      "eslint"
    ],
    "*.css": [
      "prettier --check",
      "cross-env LINT_PRECOMMIT_RUN=1 stylelint --allow-empty-input"
    ],
    "*.{json,md}": [
      "prettier --check"
    ]
  },
  "engines": {
    "yarn": "^1.21.1"
  },
  "packageManager": "yarn@1.22.19",
  "workspaces": {
    "packages": [
      ".github/actions/*",
      "e2e",
      "styleguide",
      "packages/*",
      "tools/*"
    ]
  }
}
