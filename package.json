    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@size-limit/preset-small-lib": "^4.9.1",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.1",
    "@testing-library/react-hooks": "^5.1.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/css-font-loading-module": "^0.0.4",
    "@types/jest": "^26.0.15",
    "@types/jest-image-snapshot": "^4.1.3",
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.8.5",
    "@types/webpack-dev-server": "^3.11.1",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.0.0",
    "@vkontakte/appearance": "https://github.com/VKCOM/Appearance#v9.2.0",
    "@vkontakte/eslint-config": "2.5.0",
    "@vkontakte/icons": "^1.109.0",
    "@vkontakte/vk-bridge": "^2.1.3",
    "autoprefixer": "^9.8.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-auto-import": "^1.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-imports": "^1.5.4",
    "chalk": "^4.1.0",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.2",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "eslint": "~6.8.0",
    "eslint-plugin-jest-playwright": "^0.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.0.1",
    "expect-playwright": "^0.2.6",
    "file-loader": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-image-snapshot": "^4.2.0",
    "jest-playwright-preset": "^1.4.1",
    "mini-css-extract-plugin": "^1.3.1",
    "playwright": "^1.8.0",
    "playwright-chromium": "^1.8.0",
    "playwright-core": "^1.8.0",
    "postcss": "8.2.10",
    "postcss-cli": "8.1.0",
    "postcss-csso": "5.0.0",
    "postcss-custom-properties": "10.0.0",
    "postcss-import": "13.0.0",
    "postcss-loader": "^4.1.0",
    "postcss-prefixer": "^2.1.2",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-docgen": "^5.4.0-alpha.0",
    "react-dom": "^16.8.6",
    "react-frame-component": "^3.0.0",
    "react-styleguidist": "^11.1.5",
    "shx": "^0.3.2",
    "size-limit": "^4.9.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^2.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-value-no-unknown-custom-properties": "^3.0.0",
    "typescript": "3.8",
    "wait-port": "^0.2.9",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "yargs": "^15.4.1"
  },
  "bin": {
    "generate_scheme": "./tasks/generate_scheme.js"
  },
  "peerDependencies": {
    "@vkontakte/icons": "^1.109.0",
    "@vkontakte/vk-bridge": "^2.0.2",
    "react": "^16.8.6 || ^17.0.0",
    "react-dom": "^16.8.6 || ^17.0.0"
  },
  "size-limit": [
    {
      "name": "JS",
      "path": "dist/index.js"
    },
    {
      "name": "JS, unstable",
      "path": "dist/unstable/index.js"
    },
    {
      "name": "CSS",
      "path": "dist/vkui.css",
      "webpack": false
    },
    {
      "name": "CSS, unstable",
      "path": "dist/unstable.css",
      "webpack": false
    }
  ],
  "scripts": {
    "prepare": "node -e '!process.env.VKUI_SKIP_PREPARE && require(`child_process`).spawn(`yarn`, [`build`], { stdio: `inherit` })'",
    "size": "yarn clear && concurrently 'yarn:babel' 'yarn:postcss' && size-limit",
    "size:ci": "yarn install --frozen-lockfile --ignore-scripts && yarn build:no-types",
    "styleguide": "cross-env NODE_ENV=development styleguidist server --config=styleguide/config.js",
    "styleguide:build": "cross-env NODE_ENV=production styleguidist build --config=styleguide/config.js",
    "dev": "yarn clear && concurrently \"yarn:tsc-dev\" \"yarn:babel-dev\" \"yarn:postcss-dev\"",
    "dev-cjs": "yarn clear && concurrently \"yarn:tsc-cjs-dev\" \"yarn:babel-cjs-dev\" \"yarn:postcss-dev\"",
    "dev-all": "yarn clear && concurrently \"yarn:tsc-dev\" \"yarn:babel-dev\" \"yarn:tsc-cjs-dev\" \"yarn:babel-cjs-dev\" \"yarn:postcss-dev\"",
    "postcss-bundle": "webpack --config webpack.styles.config.js",
    "postcss-modules": "postcss './src/**/*.css' --base ./src --dir ./dist/cssm",
    "postcss-dev": "concurrently 'yarn:postcss-bundle --watch' 'yarn:postcss-modules --watch'",
    "babel-base": "babel src/ --extensions .tsx,.jsx,.ts,.js",
    "babel-dev": "yarn babel-base --out-dir dist/ --watch",
    "babel-cjs-dev": "yarn babel-base --config-file ./babel.cjs.config.js --out-dir dist/cjs/ --watch",
    "tsc-dev": "tsc --emitDeclarationOnly --declaration --outDir dist/ --watch --preserveWatchOutput",
    "tsc-cjs-dev": "tsc --emitDeclarationOnly --declaration --outDir dist/cjs/ --watch --preserveWatchOutput",
    "build:types": "yarn tsc && yarn tsc-cjs",
    "build:no-types": "concurrently 'yarn:babel' 'yarn:babel-cssm' 'yarn:babel-cjs' 'yarn:postcss'",
    "build": "yarn build:types && yarn build:no-types",
    "postcss": "cross-env NODE_ENV=production concurrently 'yarn:postcss-bundle' 'yarn:postcss-modules'",
    "babel": "cross-env NODE_ENV=production yarn babel-base --out-dir dist/ --source-maps",
    "babel-cssm": "cross-env BABEL_KEEP_CSS=1 NODE_ENV=production yarn babel-base --out-dir dist/cssm --source-maps",
    "babel-cjs": "cross-env NODE_ENV=production yarn babel-base --config-file ./babel.cjs.config.js --out-dir dist/cjs/ --source-maps",
    "tsc": "cross-env NODE_ENV=production tsc --emitDeclarationOnly --declaration -p tsconfig.dist.json",
    "tsc-cjs": "cross-env NODE_ENV=production tsc --emitDeclarationOnly --declaration --outDir dist/cjs/ -p tsconfig.dist.json",
    "clear": "shx rm -rf dist/*",
    "test:unit": "jest --config jest.unit.config.js",
    "test:unit:ci": "yarn test:unit --ci --silent --outputFile test-results.json --json --coverage --coverageReporters='json' --coverageDirectory='.nyc_output'",
    "test:e2e": "jest --config e2e/jest.e2e.config.js",
    "test:e2e:ci": "yarn test:e2e --ci --outputFile e2e-results.json --json",
    "test:combine-coverage": "npx nyc report --reporter=lcov",
    "lint:es": "eslint --ext .jsx,.js,.ts,.tsx src/ e2e/ styleguide/ --cache --cache-location .cache/.eslintcache",
    "lint:es:ci": "yarn lint:es -o lint-results.json -f json",
    "lint:style": "cross-env LINT_PRECOMMIT_RUN=1 stylelint \"./src/**/*.css\" --cache --cache-location .cache/.stylelintcache",
    "typecheck": "tsc --incremental --emitDeclarationOnly --declaration --tsBuildInfoFile .cache/.tsbuildinfo --outDir .cache/ts",
    "test:static": "concurrently 'yarn typecheck' 'yarn lint:es' 'yarn lint:style'",
    "test:static:ci": "concurrently 'yarn typecheck' 'yarn lint:es:ci' 'yarn lint:style'",
    "test": "concurrently 'yarn test:unit' 'yarn test:static'",
    "test:ci": "concurrently 'yarn test:unit:ci' 'yarn test:static:ci'",
    "codesandbox:install": "PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD=1 yarn install"
  },
  "pre-commit": [
    "test:static"
  ],
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "@popperjs/core": "^2.9.2",
    "@vkontakte/vkjs": "^0.22.1",
    "react-popper": "^2.2.5"
  }
}
