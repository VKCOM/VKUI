      {/* С функцией */}
      <Alert title="Alert title" onClosed={handleClose} />
      {/* Вложенный случай */}
      <div>
        <Alert title="Nested Alert" onClosed={() => setVisible(false)} />
      </div>
    </React.Fragment>
  );
};

function handleClose() {
  console.log('closed');
